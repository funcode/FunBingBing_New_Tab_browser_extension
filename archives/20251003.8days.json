        "ultraHighDef": "/th?id=OHR.EucalyptusKoala_ZH-CN6942451940_UHD.jpg",
        "wallpaper": "/th?id=OHR.EucalyptusKoala_ZH-CN6942451940_1920x1200.jpg"
